{
  "name": "api",
  "version": "1.0.0",
  "main": "./src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --out-dir dist",
    "start": "NODE_ENV=production node dist/index.js",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed:admin": "ts-node src/seed/admin.ts",
    "db:seed:npru": "ts-node src/seed/npru.ts",
    "db:migrate:reset": "prisma migrate reset --force --skip-seed",
    "db:migrate:reset:seed": "prisma migrate reset --force --skip-seed && ts-node src/seed/admin.ts && ts-node src/seed/npru.ts",
    "db:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/jwt": "^1.3.1",
    "@elysiajs/node": "^1.3.0",
    "@elysiajs/swagger": "^1.3.0",
    "@prisma/client": "6.5.0",
    "bcryptjs": "^3.0.2",
    "elysia": "^1.3.4",
    "prisma": "6.5.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.15.3",
    "prisma-dbml-generator": "^0.12.0",
    "prismabox": "^1.1.24",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
